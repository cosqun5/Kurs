@using EntityLayer.Concrate
@model List<Əlaqə>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLyaout.cshtml";
}

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Əlaqə</h4>

            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>№</th>
                            <th>Fecabook</th>
                            <th>Instagram</th>
                            <th>Telefon</th>
                            <th>Sil</th>
                            <th>Yenilə</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.ƏlaqəID</td>
                                <td>@item.Fecabook</td>
                                <td>@item.Instagram</td>
                                <td>@item.Telefon</td>
                                <td><a href="/Əlaqə/DeleteƏlaqə/@item.ƏlaqəID" class="btn btn-outline-danger">Sil</a></td>
                                <td><a href="/Əlaqə/UpdateƏlaqə/@item.ƏlaqəID" class="btn btn-outline-success">Yenilə</a></td>
                            </tr>
                        }


                    </tbody>
                </table>

            </div>
            <br />
            <br />
            <a href="/Əlaqə/AddƏlaqə" class="btn btn-outline-info">Əlaqələri Yüklə</a>
        </div>
    </div>
</div>


